<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.iwhalecloud.retail.goods.mapper.ProdTagsMapper" >

  <select id="getTagsByGoodsId" resultType="com.iwhalecloud.retail.goods.dto.ProdTagsDTO"
          parameterType="String">
      select t1.tag_name,t1.tag_id
      from prod_tags t1
      left join prod_tag_rel t2 on t1.TAG_ID = t2.TAG_ID
      where goods_id = #{goodsId}

    </select>

    <select id="listProdTags" resultType="com.iwhalecloud.retail.goods.dto.ProdTagsDTO">
        select * from prod_tags
    </select>

</mapper>